
<h1>Cats Index</h1>




<ul>
    <%@cats.each do |cat| %>
        <a href="<%= cat_url(cat)%>">Cats Show Page</a>
        <li>
            <ul>
                <li><%= cat.birth_date%></li>
                <li><%= cat.color%></li>
                <li><%= cat.name%></li>
                <li><%= cat.sex%></li>
                 <li><%= cat.age%></li>
                <li><%= cat.description%></li>
            </ul>
        </li>
    <% end %>
</ul>